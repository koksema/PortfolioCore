@model List<Future>
<section id="hero" class="hero section light-background">

    @*<img src="/MyResume-1.0.0/assets/img/hero-bg.jpg" alt="">*@
    <img src="https://media.istockphoto.com/id/1422479114/tr/foto%C4%9Fraf/white-background-stripes-3d-wavy-pattern-elegant-abstract-striped-pattern.jpg?s=612x612&w=0&k=20&c=muRh6ofi7uR2uZmv07YRBiOxWggQbK-9zgaMcr16fk4=" />
    @foreach(var item in Model)
    {
        
    
    <div class="container" data-aos="zoom-out">
        <div class="row justify-content-center">
            <div class="col-lg-9">
                <h2>@item.NameSurname</h2>
                    <p>Çalışma Alanlarım: <span class="typed" data-typed-items="@item.Title1,@item.Title2,@item.Title3">Developer</span><span class="typed-cursor typed-cursor--blink" aria-hidden="true"></span></p>
                @await Component.InvokeAsync("_DefaultSocialMediaComponentPartial")
            </div>
        </div>
    </div>
    }
</section>   <!-- /Hero Section -->

